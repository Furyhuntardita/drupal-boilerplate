{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "https://github.com/metadrop/behat-contexts"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mistermoper/drupal-behat"
        },
        {
            "type": "package",
            "package": {
                "name": "fontawesome/fontawesome",
                "version": "5.5.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://use.fontawesome.com/releases/v5.5.0/fontawesome-free-5.5.0-web.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal-composer/drupal-security-advisories": "8.x-dev",
        "drupal/admin_toolbar": "^1.21",
        "drupal/backup_migrate": "^4.0@beta",
        "drupal/block_class": "^1.0@alpha",
        "drupal/components": "^1.1",
        "drupal/config_split": "^1.3",
        "drupal/config_update": "^1.5",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.6",
        "drupal/crop": "^1.3",
        "drupal/ctools": "^3.0",
        "drupal/environment_indicator": "^3.2",
        "drupal/eu_cookie_compliance": "^1.2",
        "drupal/google_tag": "^1.0",
        "drupal/honeypot": "^1.27",
        "drupal/metatag": "^1.3",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/radix": "^4.6",
        "drupal/redirect": "^1.0@beta",
        "drupal/simple_sitemap": "^2.11",
        "drupal/styleguide": "^1.0@alpha",
        "drupal/token": "^1.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/webform": "^5.0@beta",
        "drush/drush": "~8.0|^9.0.0-beta8",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "bex/behat-step-time-logger": "^2.0",
        "drupal/coder": "^8.2",
        "drupal/console": "^1.0.2",
        "drupal/devel": "^2.1",
        "drupal/devel_php": "^1.0",
        "drupal/drupal-extension": "^3.4",
        "drupal/reroute_email": "^1.1",
        "drupal/stage_file_proxy": "^1.0",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "metadrop/behat-contexts": "dev-master",
        "metadrop/grumphp-drupal-check": "^0.1",
        "mglaman/drupal-check": "^1.0",
        "mikey179/vfsstream": "~1.2",
        "pdepend/pdepend": "^2.5",
        "phing/phing": "^2.16",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "~6.5",
        "sebastian/phpcpd": "^3.0",
        "squizlabs/php_codesniffer": "2.x",
        "symfony/css-selector": "~2.8|~3.0",
        "symfony/phpunit-bridge": "^3.4.3",
        "vijaycs85/drupal-quality-checker": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "remove-git-submodules" : "find . -mindepth 2 -type d -name .git | xargs rm -rf",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "@remove-git-submodules"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "@remove-git-submodules"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/draggableviews": {
                "Duplicate entities when 2 different entity types have the same entity_id - https://www.drupal.org/node/2796313": "https://www.drupal.org/files/issues/duplicate_entities_when-2796313-7.patch"
            },
            "drupal/core": {
                "Entity Links fields does not have translation support - https://www.drupal.org/project/drupal/issues/2877994": "https://www.drupal.org/files/issues/2019-05-29/2877994-140.patch"
            }
        },
        "composer-exit-on-patch-failure": true
    }
}
